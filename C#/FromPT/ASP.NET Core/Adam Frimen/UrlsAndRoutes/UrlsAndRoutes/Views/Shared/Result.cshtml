@model Result
 @{ Layout = null; }
    <!DOCTYPE html>
    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Routing</title>
        <link rel="stylesheet" asp-href-include="lib/bootstrap/css/*.min.css" />
    </head>
    <body class="m-l p-l">
        <table class="table table-bordered table-striped table-sm">
            <tr><th>Controller : </th><td>@Model.Controller</td></tr>
            <tr><th>Action : </th><td>@Model.Action</td></tr>
            @foreach (string key in Model.Data.Keys)
            {
                <tr><th>@key : </th><td>@Model.Data[key] </td></tr>
            }
        </table>
        <a asp-controller="Home"asp-action="CustomVariable" asp-route-id="Hello">  This is an outgoing URL</a>
        <br>
        <a asp-controller="Admin" asp-action="Index">
            This targets another controller
        </a>
    </body>
    </html>